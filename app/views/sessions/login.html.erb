<h1> Log In Here: </h1>

<%= form_with url: "/login" do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit 'Log In' %>
<% end %>

<h3> Or Login with Google: </h3>

<%= button_to 'Google', '/auth/google_oauth2' %>